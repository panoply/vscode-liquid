<div>

  foo

  {% for block in section.blocks %}

    {% if block.type == 'something' %}

      {{ block.settings.xxxx }}

    {% elsif block.type == 'xxx' %}

      {{ block.settings.foo }}

    {% endif %}


  {% endfor %}

</div>


