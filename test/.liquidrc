{
  "engine": "11ty",
  "files": {
    "includes": [
      "./sections/*.liquid"
    ],
    "data": [
      "./11ty/data/*.json"
    ]
  },
  "format": {
    "ignore": [],
    "wrap": 120,
    "wrapFraction": 80,
    "indentSize": 2,
    "preserveLine": 2,
    "endNewline": true,
    "liquid": {
      "indentAttribute": true,
      "lineBreakSeparator": "before",
      "normalizeSpacing": true,
      "quoteConvert": "single",
      "ignoreTagList": [],
      "forceArgument": 3
    },
    "markup": {
      "quoteConvert": "double",
      "selfCloseSpace": true,
      "delimiterTerminus": "inline",
      "stripAttributeLines": true,
      "commentNewline": false,
      "forceIndent": true,
      "commentIndent": true,
      "ignoreJS": true,
      "ignoreCSS": false,
      "ignoreJSON": false,
      "forceAttribute": 3
    },
    "json": {
      "bracePadding": false,
      "braceAllman": true,
      "arrayFormat": "indent",
      "objectIndent": "indent",
      "objectSort": false
    },
    "style": {
      "noLeadZero": true,
      "quoteConvert": "single",
      "atRuleSpace": true
    }
  }
}