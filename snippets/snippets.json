{
  "Jekyll frontmatter": {
    "prefix": "--",
    "description": "Frontmatter",
    "body": ["---", "$2", "---"]
  },
  "Liquid Tag if": {
    "prefix": "if",
    "description": "Control flow tag: if",
    "body": [
      "{%${1:-} if ${2:condition} ${1:-}%}",
      "\t$3",
      "{%${1:-} endif ${1:-}%}"
    ]
  },
  "Liquid Tag else": {
    "prefix": "else",
    "description": "Control flow tag: else",
    "body": ["{%${1:-} else ${1:-}%}", "\t"]
  },
  "Liquid Tag elsif": {
    "prefix": "elsif",
    "description": "Control flow tag: elsif",
    "body": ["{%${1:-} elsif ${2:condition} ${1:-}%}", "\t"]
  },
  "Liquid Tag if, elsif": {
    "prefix": "ifelse",
    "description": "Control flow tag: if else",
    "body": [
      "{%${1:-} if ${2:condition} ${1:-}%}",
      "\t$3",
      "{%${1:-} elsif ${4:condition} ${1:-}%}",
      "\t$5",
      "{%${1:-} endif ${1:-}%}"
    ]
  },
  "Liquid Tag if, else": {
    "prefix": "ifelse",
    "description": "Control flow tag: if else",
    "body": [
      "{%${1:-} if ${2:condition} ${1:-}%}",
      "\t$3",
      "{%${1:-} else ${1:-}%}",
      "\t$4",
      "{%${1:-} endif ${1:-}%}"
    ]
  },
  "Liquid Tag unless": {
    "prefix": "unless",
    "description": "Control flow tag: unless",
    "body": [
      "{%${1:-} unless ${2:condition} ${1:-}%}",
      "\t$3",
      "{%${1:-} endunless ${1:-}%}"
    ]
  },
  "Liquid Tag case, else": {
    "prefix": "case",
    "description": "Control flow tag: case",
    "body": [
      "{%${1:-} case ${2:variable} ${1:-}%}",
      "\t{%${1:-} when ${3:condition} ${1:-}%}",
      "\t\t$4",
      "\t{%${1:-} else ${1:-}%}",
      "\t\t$5",
      "{%${1:-} endcase ${1:-}%}"
    ]
  },
  "Liquid Tag when": {
    "prefix": "when",
    "description": "Control flow tag: when",
    "body": ["{%${1:-} when ${2:condition} ${1:-}%}", "$0"]
  },
  "Liquid Tag cycle": {
    "prefix": "cycle",
    "description": "Iteration tag: cycle",
    "body": [
      "{%${1:-} cycle '${2:odd}', '${3:even}' ${1:-}%}"
    ]
  },
  "Liquid Tag cycle group": {
    "prefix": "cyclegroup",
    "description": "Iteration tag: cycle group",
    "body": [
      "{%${1:-} cycle '${2:group name}': '${3:odd}', '${4:even}' ${1:-}%}"
    ]
  },
  "Liquid Tag for": {
    "prefix": "for",
    "description": "Iteration tag: for",
    "body": [
      "{%${1:-} for ${2:item} in ${3:collection} ${1:-}%}",
      "\t$4",
      "{%${1:-} endfor ${1:-}%}"
    ]
  },
  "Liquid Tag Option limit": {
    "prefix": "limit",
    "description": "For loops option",
    "body": ["limit: ${1:5}"]
  },
  "Liquid Tag Option offset": {
    "prefix": "offset",
    "description": "For loops option",
    "body": ["offset: ${1:0}"]
  },
  "Liquid Tag Option reversed": {
    "prefix": "reversed",
    "description": "For loops option",
    "body": ["reversed"]
  },
  "Liquid Tag break": {
    "prefix": "break",
    "description": "Iteration tag: break",
    "body": ["{%${1:-} break ${1:-}%}"]
  },
  "Liquid Tag continue": {
    "prefix": "continue",
    "description": "Iteration tag: continue",
    "body": ["{%${1:-} continue ${1:-}%}"]
  },
  "Liquid Tag tablerow": {
    "prefix": "tablerow",
    "description": "Iteration tag: tablerow",
    "body": [
      "{%${1:-} tablerow ${2:item} in ${3:collection} cols: ${4:2} ${1:-}%}",
      "\t$5",
      "{%${1:-} endtablerow ${1:-}%}"
    ]
  },
  "Liquid Tag assign": {
    "prefix": "assign",
    "description": "Variable tag: assign",
    "body": [
      "{%${1:-} assign ${2:variable} = ${3:value} ${1:-}%}"
    ]
  },
  "Liquid Tag increment": {
    "prefix": "increment",
    "description": "Variable tag: increment",
    "body": ["{%${1:-} increment ${2:variable} ${1:-}%}"]
  },
  "Liquid Tag decrement": {
    "prefix": "decrement",
    "description": "Variable tag: decrement",
