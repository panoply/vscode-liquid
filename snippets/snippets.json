    "description": "Schema input setting: \"header\"",
    "prefix": "schema: header",
    "body": [
      "{",
      "\t\"type\": \"header\",",
      "\t\"content\": \"$1\"${2:,\n\t\"info\": \"$3\"}${4:,}",
      "}${5:,}$0"
    ]
  },
