  "Schema, name": {
    "description": "Schema property: \"name\"",
    "prefix": "schema: name",
    "body": "\"name\": \"$1\"$0"
  },
  "Schema, tag": {
    "description": "Schema property: \"tag\"",
    "prefix": "schema: tag",
    "body": "\"tag\": \"${1|article,aside,div,footer,header,section|}\",${2:,}"
  },
  "Schema, class": {
    "description": "Schema property: \"class\"",
    "prefix": "schema: class",
    "body": "\"class\": \"$1\"${2:,}$0"
  },
  "Schema, settings": {
    "prefix": "schema: settings",
