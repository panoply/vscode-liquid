  "Liquid Tag javascript": {
    "prefix": "javascript",
    "description": "Javascript tag: javascript",
    "body": [
      "{%${1:-} javascript ${1:-}%}",
      "\t$1",
      "{%${1:-} endjavascript ${1:-}%}"
    ]
  },
  "Liquid Tag comment, inline": {
    "prefix": "#",
    "description": "Comment tag: comment, inline",
    "body": ["{%${1:-} # $2 ${1:-}%}"]
  },

