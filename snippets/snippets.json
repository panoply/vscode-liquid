  },
  "Liquid Tag form": {
    "prefix": "form",
    "description": "Theme tag: form",
    "body": [
      "{%${1:-} form '${2:product}', ${3:product} ${1:-}%}",
      "\t$4",
      "{%${1:-} endform ${1:-}%}"
    ]
  },
  "Liquid Tag paginate": {
    "prefix": "paginate",
    "description": "Theme tag: paginate",
    "body": [
      "{%${1:-} paginate ${1:collection.products} by ${2:12} ${1:-}%}",
      "\t{%${1:-} for ${3:product} in ${1:collection.products} ${1:-}%}",
      "\t\t$4",
      "\t{%${1:-} endfor ${1:-}%}",
      "{%${1:-} endpaginate ${1:-}%}"
    ]
  },
  "Liquid Tag schema": {
    "prefix": "schema",
    "description": "Section Tag: schema",
    "body": [
      "{%${1:-} schema ${1:-}%}",
      "{",
      "\t$0",
      "}",
      "{%${1:-} endschema ${1:-}%}"
    ]
  },
  "Liquid Tag style": {
    "prefix": "style",
    "description": "Style tag: style",
    "body": [
