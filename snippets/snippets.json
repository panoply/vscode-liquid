{
  "Jekyll frontmatter": {
    "prefix": "--",
    "description": "Frontmatter",
    "body": ["---", "$2", "---"]
  },
  "{% tag %}, if": {
    "prefix": "if",
    "description": "Control flow tag: if",
    "body": [
      "{%${1:-} if ${2:condition} ${1:-}%}",
      "\t$3",
      "{%${1:-} endif ${1:-}%}"
    ]
  },
  "{% tag %}, else": {
    "prefix": "else",
    "description": "Control flow tag: else",
    "body": ["{%${1:-} else ${1:-}%}", "\t"]
  },
  "{% tag %}, elsif": {
    "prefix": "elsif",
    "description": "Control flow tag: elsif",
    "body": ["{%${1:-} elsif ${2:condition} ${1:-}%}", "\t"]
  },
  "{% tag %}, if, elsif": {
    "prefix": "ifelse",
    "description": "Control flow tag: if else",
    "body": [
      "{%${1:-} if ${2:condition} ${1:-}%}",
      "\t$3",
      "{%${1:-} elsif ${4:condition} ${1:-}%}",
      "\t$5",
      "{%${1:-} endif ${1:-}%}"
    ]
  },
  "{% tag %}, if, else": {
    "prefix": "ifelse",
    "description": "Control flow tag: if else",
    "body": [
      "{%${1:-} if ${2:condition} ${1:-}%}",
      "\t$3",
      "{%${1:-} else ${1:-}%}",
      "\t$4",
      "{%${1:-} endif ${1:-}%}"
    ]
  },
  "{% tag %}, unless": {
    "prefix": "unless",
    "description": "Control flow tag: unless",
    "body": [
      "{%${1:-} unless ${2:condition} ${1:-}%}",
      "\t$3",
      "{%${1:-} endunless ${1:-}%}"
    ]
  },
  "{% tag %}, case, else": {
    "prefix": "case",
    "description": "Control flow tag: case",
    "body": [
      "{%${1:-} case ${2:variable} ${1:-}%}",
      "\t{%${1:-} when ${3:condition} ${1:-}%}",
      "\t\t$4",
      "\t{%${1:-} else ${1:-}%}",
      "\t\t$5",
      "{%${1:-} endcase ${1:-}%}"
    ]
  },
  "{% tag %}, when": {
    "prefix": "when",
    "description": "Control flow tag: when",
    "body": ["{%${1:-} when ${2:condition} ${1:-}%}", "$0"]
  },
  "{% tag %}, cycle": {
    "prefix": "cycle",
    "description": "Iteration tag: cycle",
    "body": [
      "{%${1:-} cycle '${2:odd}', '${3:even}' ${1:-}%}"
    ]
  },
  "{% tag %}, cycle group": {
    "prefix": "cyclegroup",
    "description": "Iteration tag: cycle group",
    "body": [
      "{%${1:-} cycle '${2:group name}': '${3:odd}', '${4:even}' ${1:-}%}"
    ]
  },
  "{% tag %}, for": {
    "prefix": "for",
    "description": "Iteration tag: for",
    "body": [
      "{%${1:-} for ${2:item} in ${3:collection} ${1:-}%}",
      "\t$4",
      "{%${1:-} endfor ${1:-}%}"
    ]
  },
  "{% tag %}, Option limit": {
    "prefix": "limit",
    "description": "For loops option",
    "body": ["limit: ${1:5}"]
  },
  "{% tag %}, Option offset": {
    "prefix": "offset",
    "description": "For loops option",
    "body": ["offset: ${1:0}"]
  },
  "{% tag %}, Option reversed": {
    "prefix": "reversed",
    "description": "For loops option",
    "body": ["reversed"]
  },
  "{% tag %}, break": {
    "prefix": "break",
    "description": "Iteration tag: break",
    "body": ["{%${1:-} break ${1:-}%}"]
  },
  "{% tag %}, continue": {
    "prefix": "continue",
    "description": "Iteration tag: continue",
    "body": ["{%${1:-} continue ${1:-}%}"]
  },
  "{% tag %}, tablerow": {
    "prefix": "tablerow",
    "description": "Iteration tag: tablerow",
    "body": [
      "{%${1:-} tablerow ${2:item} in ${3:collection} cols: ${4:2} ${1:-}%}",
      "\t$5",
      "{%${1:-} endtablerow ${1:-}%}"
    ]
  },
  "{% tag %}, assign": {
    "prefix": "assign",
    "description": "Variable tag: assign",
    "body": [
      "{%${1:-} assign ${2:variable} = ${3:value} ${1:-}%}"
    ]
  },
  "{% tag %}, increment": {
    "prefix": "increment",
    "description": "Variable tag: increment",
    "body": ["{%${1:-} increment ${2:variable} ${1:-}%}"]
  },
  "{% tag %}, decrement": {
    "prefix": "decrement",
    "description": "Variable tag: decrement",
    "body": ["{%${1:-} decrement ${2:variable} ${1:-}%}"]
  },
  "{% tag %}, capture": {
    "prefix": "capture",
    "description": "Variable tag: capture",
    "body": [
      "{%${1:-} capture ${2:variable} ${1:-}%}",
      "\t$3",
      "{%${1:-} endcapture ${1:-}%}"
    ]
  },
  "{% tag %}, liquid": {
    "prefix": "liquid",
    "description": "{% tag %},: liquid",
    "body": ["{%${1:-} liquid", "\t$2", "${1:-}%}"]
  },
  "{% tag %}, render": {
    "prefix": "render",
    "description": "Theme tag: render",
    "body": ["{%${1:-} render '${2:snippet}' ${1:-}%}"]
  },
  "{% tag %}, include": {
    "prefix": "include",
    "description": "Theme tag: include",
    "body": ["{%${1:-} include '${2:snippet}' ${1:-}%}"]
  },
  "{% tag %}, include with parameters": {
    "prefix": "includewith",
    "description": "Theme tag: include with parameters",
    "body": [
      "{%${1:-} include '${2:snippet}', ${3:variable}: ${4:value} ${1:-}%}"
    ]
  },
  "{% tag %}, section": {
    "prefix": "section",
    "description": "Theme tag: section",
    "body": ["{%${1:-} section '${2:section}' ${1:-}%}"]
  },
  "{% tag %}, raw": {
    "prefix": "raw",
    "description": "Theme tag: raw",
    "body": [
      "{%${1:-} raw ${1:-}%}",
      "\t$2",
      "{%${1:-} endraw ${1:-}%}"
    ]
  },
  "{% tag %}, layout": {
    "prefix": "layout",
    "description": "Theme tag: layout",
    "body": ["{%${1:-} layout '${2:layout}' ${1:-}%}"]
  },
  "{% tag %}, no layout": {
    "prefix": "layoutnone",
    "description": "Theme tag: layout none",
    "body": ["{%${1:-} layout none ${1:-}%}"]
  },
  "{% tag %}, form": {
    "prefix": "form",
    "description": "Theme tag: form",
    "body": [
      "{%${1:-} form '${2:product}', ${3:product} ${1:-}%}",
      "\t$4",
      "{%${1:-} endform ${1:-}%}"
    ]
  },
  "{% tag %}, paginate": {
    "prefix": "paginate",
    "description": "Theme tag: paginate",
    "body": [
      "{%${1:-} paginate ${1:collection.products} by ${2:12} ${1:-}%}",
      "\t{%${1:-} for ${3:product} in ${1:collection.products} ${1:-}%}",
      "\t\t$4",
      "\t{%${1:-} endfor ${1:-}%}",
      "{%${1:-} endpaginate ${1:-}%}"
    ]
  },
  "{% tag %}, schema": {
    "prefix": "schema",
    "description": "Section Tag: schema",
    "body": [
      "{%${1:-} schema ${1:-}%}",
      "{",
      "\t$0",
      "}",
      "{%${1:-} endschema ${1:-}%}"
    ]
  },
  "{% tag %}, style": {
    "prefix": "style",
    "description": "Style tag: style",
    "body": [
      "{%${1:-} style ${1:-}%}",
      "\t$0",
      "{%${1:-} endstyle ${1:-}%}"
    ]
  },
  "{% tag %}, stylesheet": {
    "prefix": "stylesheet",
    "description": "Stylesheet tag: stylesheet",
    "body": [
      "{%${1:-} stylesheet ${1:-}%}",
      "\t$0",
      "{%${1:-} endstylesheet ${1:-}%}"
    ]
  },
  "{% tag %}, javascript": {
    "prefix": "javascript",
    "description": "Javascript tag: javascript",
    "body": [
      "{%${1:-} javascript ${1:-}%}",
      "\t$1",
      "{%${1:-} endjavascript ${1:-}%}"
    ]
  },
  "{% tag %}, comment, inline": {
    "prefix": "#",
    "description": "Comment tag: comment, inline",
    "body": ["{%${1:-} # $2 ${1:-}%}"]
  },

  "Filter join": {
    "prefix": "join",
    "description": "Array filter: join",
    "body": "${1:|} join: '${2:, }'"
  },
  "Filter first": {
    "prefix": "first",
    "description": "Array filter: first",
    "body": "${1:|} first"
  },
  "Filter last": {
    "prefix": "last",
    "description": "Array filter: last",
    "body": "${1:|} last"
  },
  "Filter concat": {
    "prefix": "concat",
    "description": "Array filter: concat",
    "body": "${1:|} concat: ${2:array}"
  },
  "Filter map": {
    "prefix": "map",
    "description": "Array filter: map",
    "body": "${1:|} map: '${2:key}'"
  },
  "Filter reverse": {
    "prefix": "reverse",
    "description": "Array filter or String filter: reverse",
    "body": "${1:|} reverse"
  },
  "Filter size": {
    "prefix": "size",
    "description": "Array filter: size",
    "body": "${1:|} size"
  },
  "Filter sort": {
    "prefix": "sort",
    "description": "Array filter: sort",
    "body": "${1:|} sort: '${2:key}'"
  },
  "Filter uniq": {
    "prefix": "uniq",
    "description": "Array filter: uniq",
    "body": "${1:|} uniq"
  },
  "Filter img tag": {
    "prefix": "img_tag",
    "description": "HTML filter: img tag",
    "body": "${1:|} img_tag"
  },
  "Filter img tag with parameters": {
    "prefix": "img_tag_param",
    "description": "HTML filter: img tag with parameters",
    "body": "${1:|} img_tag: '${2:alt}', '${3:class}'"
  },
  "Filter script tag": {
    "prefix": "script_tag",
    "description": "HTML filter: script tag",
    "body": "${1:|} script_tag"
  },
  "Filter stylesheet tag": {
    "prefix": "stylesheet_tag",
    "description": "HTML filter: stylesheet tag",
    "body": "${1:|} stylesheet_tag"
  },
  "Filter abs": {
    "prefix": "abs",
    "description": "Math filter: abs",
    "body": "${1:|} abs"
  },
  "Filter ceil": {
    "prefix": "ceil",
    "description": "Math filter: ceil",
    "body": "${1:|} ceil"
  },
  "Filter divided by": {
    "prefix": "divided_by",
    "description": "Math filter: divided by",
    "body": "${1:|} divided_by: ${2:2}"
  },
  "Filter floor": {
    "prefix": "floor",
    "description": "Math filter: floor",
    "body": "${1:|} floor"
  },
  "Filter minus": {
    "prefix": "minus",
    "description": "Math filter: minus",
    "body": "${1:|} minus: ${2:1}"
  },
  "Filter plus": {
    "prefix": "plus",
    "description": "Math filter: plus",
    "body": "${1:|} plus: ${2:1}"
  },
  "Filter round": {
    "prefix": "round",
    "description": "Math filter: round",
    "body": "${1:|} round: ${2:0}"
  },
  "Filter times": {
    "prefix": "times",
    "description": "Math filter: times",
    "body": "${1:|} times: ${2:1}"
  },
  "Filter modulo": {
    "prefix": "modulo",
    "description": "Math filter: modulo",
    "body": "${1:|} modulo: ${2:2}"
  },
  "Filter money": {
    "prefix": "money",
    "description": "Money filter: money",
    "body": "${1:|} money"
  },
  "Filter money with currency": {
    "prefix": "money_with_currency",
    "description": "Money filter: money with currency",
    "body": "${1:|} money_with_currency"
  },
  "Filter money without trailing zeros": {
    "prefix": "money_without_trailing_zeros",
    "description": "Money filter: money without trailing zeros",
    "body": "${1:|} money_without_trailing_zeros"
  },
  "Filter money without currency": {
    "prefix": "money_without_currency",
    "description": "Money filter: money without currency",
    "body": "${1:|} money_without_currency"
  },
  "Filter append": {
    "prefix": "append",
    "description": "String filter: append",
    "body": "${1:|} append: '${2:string}'"
  },
  "Filter camelcase": {
    "prefix": "camelcase",
    "description": "String filter: camelcase",
    "body": "${1:|} camelcase"
  },
  "Filter capitalize": {
    "prefix": "capitalize",
    "description": "String filter: capitalize",
    "body": "${1:|} capitalize"
  },
  "Filter downcase": {
    "prefix": "downcase",
    "description": "String filter: downcase",
    "body": "${1:|} downcase"
  },
  "Filter escape": {
    "prefix": "escape",
    "description": "String filter: escape",
    "body": "${1:|} escape"
  },
  "Filter handleize": {
    "prefix": "handleize",
    "description": "String filter: handleize",
    "body": "${1:|} handleize"
  },
  "Filter md5": {
    "prefix": "md5",
    "description": "String filter: md5",
    "body": "${1:|} md5"
  },
  "Filter newline to br": {
    "prefix": "newline_to_br",
    "description": "String filter: newline to br",
    "body": "${1:|} newline_to_br"
  },
  "Filter pluralize": {
    "prefix": "pluralize",
    "description": "String filter: pluralize",
    "body": "${1:|} pluralize: '${2:item}', '${3:items}'"
  },
  "Filter prepend": {
    "prefix": "prepend",
    "description": "String filter: prepend",
    "body": "${1:|} prepend: '${2:string}'"
  },
  "Filter remove": {
    "prefix": "remove",
    "description": "String filter: remove",
    "body": "${1:|} remove: '${2:string}'"
  },
  "Filter remove first": {
    "prefix": "remove_first",
    "description": "String filter: remove first",
    "body": "${1:|} remove_first: '${2:string}'"
  },
  "Filter replace": {
    "prefix": "replace",
    "description": "String filter: replace",
    "body": "${1:|} replace: '${2:target}', '${3:replace}'"
  },
  "Filter replace first": {
    "prefix": "replace_first",
    "description": "String filter: replace first",
    "body": "${1:|} replace_first: '${2:target}', '${3:replace}'"
  },
  "Filter slice": {
    "prefix": "slice",
    "description": "String filter: slice",
    "body": "${1:|} slice: ${2:0}, ${3:5}"
  },
  "Filter slice single character": {
    "prefix": "slice_single",
    "description": "String filter: slice with single parameter",
    "body": "${1:|} slice: ${2:at}"
  },
  "Filter split": {
    "prefix": "split",
    "description": "String filter: split",
    "body": "${1:|} split: '${2:,}'"
  },
  "Filter strip": {
    "prefix": "strip",
    "description": "String filter: strip",
    "body": "${1:|} strip"
  },
  "Filter lstrip": {
    "prefix": "lstrip",
    "description": "String filter: lstrip",
    "body": "${1:|} lstrip"
  },
  "Filter rstrip": {
    "prefix": "rstrip",
    "description": "String filter: rstrip",
    "body": "${1:|} rstrip"
  },
  "Filter strip html": {
    "prefix": "strip_html",
    "description": "String filter: strip html",
    "body": "${1:|} strip_html"
  },
  "Filter strip newlines": {
    "prefix": "strip_newlines",
    "description": "String filter: strip newlines",
    "body": "${1:|} strip_newlines"
  },
  "Filter truncate": {
    "prefix": "truncate",
    "description": "String filter: truncate",
    "body": "${1:|} truncate: ${2:20}, '${3:...}'"
  },
  "Filter truncatewords": {
    "prefix": "truncatewords",
    "description": "String filter: truncatewords",
    "body": "${1:|} truncatewords: ${2:5}, '${3:...}'"
  },
  "Filter upcase": {
    "prefix": "upcase",
    "description": "String filter: upcase",
    "body": "${1:|} upcase"
  },
  "Filter url encode": {
    "prefix": "url_encode",
    "description": "String filter: url encode",
    "body": "${1:|} url_encode"
  },
  "Filter url escape": {
    "prefix": "url_escape",
    "description": "String filter: url escape",
    "body": "${1:|} url_escape"
  },
  "Filter url param escape": {
    "prefix": "url_param_escape",
    "description": "String filter: url param escape",
    "body": "${1:|} url_param_escape"
  },
  "Filter asset url": {
    "prefix": "asset_url",
    "description": "URL filter: asset url",
    "body": "${1:|} asset_url"
  },
  "Filter asset img url": {
    "prefix": "asset_img_url",
    "description": "URL filter: asset img url",
    "body": "${1:|} asset_img_url: '${2:medium}'"
  },
  "Filter img url": {
    "prefix": "img_url",
    "description": "URL filter: img url",
    "body": "${1:|} img_url: '${2:medium}'"
  }
}
