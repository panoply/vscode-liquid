  "comment, line": {
    "prefix": "#",
    "body": [
      "{% # $2 %}"
    ]
  },
  "comment-, line-": {
    "prefix": "#",
    "body": [
      "{%- # $2 -%}"
    ]
  },
  "comment, block": {
    "prefix": "comment",
    "body": [
      "{% comment %}",
      "\t${1|esthetic-ignore,esthetic-ignore-start,esthetic-ignore-end,esthetic-ignore-next|}",
      "{% endcomment %}"
    ]
  },
  "comment-, block-": {
    "prefix": "comment-",
    "body": [
      "{%- comment -%}",
      "\t${1|esthetic-ignore,esthetic-ignore-start,esthetic-ignore-end,esthetic-ignore-next|}",
      "{%- endcomment -%}"
    ]
  },
  "esthetic-ignore": {
    "prefix": "esthetic-",
    "body": [
      "{%- # esthetic-ignore -%}"
    ]
  },
  "esthetic-ignore-next": {
    "prefix": "esthetic-",
    "body": [
      "{%- # esthetic-ignore-next -%}"
    ]
  },
  "esthetic-ignore-start": {
    "prefix": "esthetic-",
    "body": [
      "{%- comment -%}esthetic-ignore-start{%- endcomment -%}"
    ]
  },
  "esthetic-ignore-end": {
    "prefix": "esthetic-",
    "body": [
      "{% comment -%}esthetic-ignore-end{%- endcomment -%}"
