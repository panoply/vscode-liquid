      "\t\t$4",
      "\t{% endfor %}",
      "{%- endpaginate -%}"
    ]
  },
  "schema": {
    "prefix": "schema",
    "body": [
      "{% schema %}",
      "{",
      "\t\"name\": \"$1\",",
      "\t\"settings\": [",
      "\t\t$0",
      "\t\t]",
      "}",
      "{% endschema %}"
    ]
  },
  "schema-": {
    "prefix": "schema-",
    "body": [
      "{%- schema -%}",
      "{",
