    ]
  },
  "form": {
    "prefix": "form",
    "body": [
      "{% form '${1:name}', ${2:product} %}",
      "\t$3",
      "{% endform %}"
    ]
  },
  "form-": {
    "prefix": "form-",
    "body": [
      "{%- form '${1:name}', ${2:product} -%}",
