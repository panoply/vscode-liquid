    ]
  },
  "increment": {
    "prefix": "increment",
    "body": [
      "{% increment ${1:variable} %}"
    ]
  },
  "increment-": {
    "prefix": "increment-",
    "body": [
      "{%- increment ${1:variable} -%}"
    ]
  },
  "decrement": {
    "prefix": "decrement",
    "body": [
      "{% decrement ${1:variable} %}"
    ]
  },
  "decrement-": {
    "prefix": "decrement-",
    "body": [
      "{%- decrement ${1:variable} -%}"
    ]
  },
  "capture": {
    "prefix": "capture",
    "body": [
      "{% capture ${1:variable} %}",
      "\t$2",
      "{% endcapture %}"
    ]
  },
  "capture-": {
    "prefix": "capture-",
    "body": [
      "{%- capture ${1:variable} -%}",
      "\t$2",
      "{%- endcapture -%}"
    ]
  },
  "liquid": {
    "prefix": "liquid",
    "body": [
      "{% liquid %}",
      "\t$1",
      "{% endliquid %}"
    ]
  },
  "liquid-": {
    "prefix": "liquid-",
    "body": [
      "{%- liquid -%}",
      "\t$1",
      "{%- endliquid -%}"
    ]
  },
  "render": {
    "prefix": "render",
    "body": [
      "{% render $1 %}"
    ]
  },
  "render-": {
    "prefix": "render-",
    "body": [
      "{%- render $1 -%}"
    ]
  },
  "render, parameters": {
    "prefix": "render",
    "body": [
      "{% render $1, ${2:param}: ${3:value} %}"
    ]
  },
  "render-, parameters-": {
    "prefix": "render-",
    "body": [
      "{%- render $1, ${2:param}: ${3:value} -%}"
    ]
  },
  "include": {
    "prefix": "include",
    "body": [
      "{% include $1 %}"
    ]
  },
  "include-": {
    "prefix": "include-",
    "body": [
      "{%- include $1 -%}"
    ]
  },
  "include, parameters": {
    "prefix": "includewith",
    "body": [
      "{% include 1, ${2:param}: ${3:value} %}"
    ]
  },
  "include-, parameters-": {
    "prefix": "includewith-",
    "body": [
      "{%- include 1, ${2:param}: ${3:value} -%}"
    ]
  },
  "section": {
    "prefix": "section",
    "body": [
      "{% section $1 %}"
    ]
  },
  "section-": {
    "prefix": "section-",
    "body": [
