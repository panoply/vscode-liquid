    "body": [
      "{% style %}",
      "\t$1",
      "{% endstyle %}"
    ]
  },
  "style-": {
    "prefix": "style-",
    "body": [
      "{%- style -%}",
      "\t$1",
      "{%- endstyle -%}"
    ]
  },
  "stylesheet": {
    "prefix": "stylesheet",
    "body": [
      "{% stylesheet %}",
      "\t$1",
      "{% endstylesheet %}"
    ]
  },
  "stylesheet-": {
