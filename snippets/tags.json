      "{%- endform -%}"
    ]
  },
  "paginate": {
    "prefix": "paginate",
    "body": [
      "{% paginate ${1:collection.products} by ${2:12} %}",
      "\t{% for ${3:product} in ${1:collection.products} %}",
      "\t\t$4",
      "\t{% endfor %}",
      "{% endpaginate %}"
    ]
  },
  "paginate-": {
    "prefix": "paginate-",
    "body": [
      "{%- paginate ${1:collection.products} by ${2:12} -%}",
